{
    "name": "Snap4Arduino connector",
    "description": "Connect Berkeley Snap! to Arduino boards",
    "version": "0.1",
    "manifest_version": 2,
    "app": {
        "background": {
            "page": "background.html"
        }
    },
    "permissions": [
        "serial"
    ],
    "externally_connectable": {
        "matches": ["http://localhost:8080/*"]
    },
    "icons": { "16": "icon-16.png", "128": "icon-128.png" }
}
